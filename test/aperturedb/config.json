    // Network
    "port": 55555,
    "max_simultaneous_clients": 100,

    // Database paths
    "db_root_path": "db",
    "log_path" : "log",

    "kvgd_db_size": 204800,

    "more-info": "ApertureDB Server",
    "verbose_log_level": 6
}
